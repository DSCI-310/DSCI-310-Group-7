     "output_type": "execute_result"
    }
   ],
   "source": [
    "# Finding K value for KNN\n",
    "k_vals = list(range(1, 21))\n",
    "param_grid = dict(n_neighbors=k_vals)\n",
    "grid = GridSearchCV(knn, param_grid, cv=4, scoring='accuracy')\n",
    "grid.fit(zoo_train_X, zoo_train_y)\n",
    "grid.best_params_"
   ]
